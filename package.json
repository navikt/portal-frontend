            }
        },
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "transform": {
            ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform",
            "^.+\\.tsx?$": "ts-jest"
        },
        "moduleNameMapper": {
            "nav-(.*)-style": "<rootDir>/src/__mocks__/mockfile.ts",
            ".*.css": "<rootDir>/src/__mocks__/styleMock.ts"
        },
        "testMatch": [
            "**/*.(test|spec).(ts|tsx|js|jsx)"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false
    }
